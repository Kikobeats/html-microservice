{
  "name": "microlink-html",
  "proxy": {
    "command": "node index.js",
    "timeout": 25
  },
  "hooks": {
    "predeploy": [
      "untracked > .upignore"
    ],
    "postdeploy": [
      "npx lambda-prune"
    ],
    "clean": [
      "rm -f .upignore"
    ]
  },
  "environment": {
    "NODE_ENV": "production",
    "TZ": "UTC"
  },
  "lambda": {
    "runtime": "nodejs8.10",
    "accelerate": true
  },
  "regions": [
    "us-east-1"
  ]
}
